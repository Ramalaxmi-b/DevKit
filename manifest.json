{
  "manifest_version": 3,
  "name": "Devkit",
  "version": "1.0",
  "description": 
  "window resizer is useful to resize your window",
  "permissions": [
      "activeTab"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "logo.jpeg",
          "48": "logo1.jpeg",
          "128": "logo2.jpeg"
      }
  },
  "icons": {
      "16": "logo.jpeg",
      "48": "logo1.jpeg",
      "128": "logo2.jpeg"
  }
}

  